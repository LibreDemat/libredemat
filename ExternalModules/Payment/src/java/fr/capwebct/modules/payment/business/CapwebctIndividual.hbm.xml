<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fr.capwebct.modules.payment.business">

  <class name="CapwebctIndividual" table="capwebct_individual" lazy="false">

    <id name="id">
      <generator class="native" />
    </id>

    <property name="capwebctIndividualId" column="capwebct_individual_id" />
    <property name="firstName" column="first_name" />
    <property name="lastName" column="last_name" />
    <property name="child" column="child" />
    <property name="responsible" column="responsible" />
    
  </class>

</hibernate-mapping>