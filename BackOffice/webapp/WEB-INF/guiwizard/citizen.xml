<?xml version="1.0" encoding="UTF-8" ?>

<cvqmanager xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="managers.xsd" >
					
  <processes group="citizen" caption="Gestion des comptes" icon="pictoAccount" order="1">
		
	<process name="eCitizen">
		<label>Cr√©ation de compte</label>
		<record>fr.cg95.cvq.bo.citizen.VoCardRequestRecord</record>
		<persistence>fr.cg95.cvq.bo.citizen.VoCardPersistence</persistence>
		
	  	<state name="display">
			<form>/jsp/bo/process/displayrequester</form>
			<form scope="request">/jsp/bo/process/citizen/displaycitizen</form>
	  	</state>
	  	<state name="check">
			<form scope="request">/jsp/bo/process/citizen/checkcitizen</form>
	  	</state>
	  	<state name="print">
			<form scope="request">/jsp/bo/process/citizen/printcitizen</form>
	  	</state>
	</process>
	<process name="eFamily">
		<label>Modification de compte</label>
		<record>fr.cg95.cvq.bo.citizen.VoCardRequestRecord</record>
		<persistence>fr.cg95.cvq.bo.citizen.VoCardPersistence</persistence>
		
	  	<state name="display">
			<form>/jsp/bo/process/displayrequester</form>
			<form scope="request">/jsp/bo/process/citizen/displaycitizen</form>
		</state>
	  	<state name="check">
			<form scope="request">/jsp/bo/process/citizen/checkcitizen</form>
	  	</state>
	  	<state name="print">
			<form scope="request">/jsp/bo/process/citizen/printcitizen</form>
	  	</state>
	</process>
  </processes>
  
</cvqmanager>
					
