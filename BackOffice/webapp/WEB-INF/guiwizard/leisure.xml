<?xml version="1.0" encoding="UTF-8" ?>

<cvqmanager xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="managers.xsd" >
					
  <processes group="leisure" caption="Loisir" icon="pictoLeisure" order="7">
	<process name="eMusic">
		<label>Inscription école de musique</label>
		<record>fr.cg95.cvq.bo.leisure.MusicSchoolRegistrationRequestRecord</record>
		<persistence>fr.cg95.cvq.bo.leisure.MusicSchoolPersistence</persistence>
		
	  	<state name="display">
			<form>/jsp/bo/process/displayrequester</form>
			<form scope="request">/jsp/bo/process/leisure/displaymusic</form>
		</state>
	  	<state name="check">
			<form scope="request">/jsp/bo/process/leisure/checkmusic</form>
		</state>
	  	<state name="print">
			<form scope="request">/jsp/bo/process/leisure/printmusic</form>
		</state>
	</process>

	<process name="eSms">
	  <label>Inscription à l'infolettre Sms</label>
	  <record>fr.cg95.cvq.bo.leisure.SmsNotificationRequestRecord</record>
	  <persistence>fr.cg95.cvq.bo.leisure.SmsNotificationPersistence</persistence>
	  <state name="display">
	    <form scope="request">/jsp/bo/process/leisure/displaysms</form>
	    <form>/jsp/bo/process/displayfooter</form>
	  </state>
	  <state name="check">
	    <form scope="request">/jsp/bo/process/leisure/checksms</form>
	  </state>
	  <state name="print">
	    <form scope="request">/jsp/bo/process/leisure/printsms</form>
	  </state>
	</process>

  </processes>
  
</cvqmanager>
					
