<?xml version="1.0" encoding="UTF-8" ?>

<cvqmanager xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xsi:noNamespaceSchemaLocation="managers.xsd" >

  <processes group="environment" caption="Environnement" icon="pictoEnvironment" order="11">
  
    <process name="eBulky">
      <label>Collecte encombrants</label>
      <record>fr.cg95.cvq.bo.environment.BulkyWasteCollectionRequestRecord</record>
      <persistence>fr.cg95.cvq.bo.environment.BulkyWasteCollectionPersistence</persistence>

      <state name="display">
        <form>/jsp/bo/process/displayrequester</form>
        <form scope="request">/jsp/bo/process/environment/displaybulky</form>
      </state>
      <state name="check">
        <form scope="request">/jsp/bo/process/environment/checkbulky</form>
      </state>
      <state name="print">
        <form scope="request">/jsp/bo/process/environment/printbulky</form>
      </state>
    </process>

    <process name="eCompostable">
      <label>Collecte déchêts verts</label>
      <record>fr.cg95.cvq.bo.environment.CompostableWasteCollectionRequestRecord</record>
      <persistence>fr.cg95.cvq.bo.environment.CompostableWasteCollectionPersistence</persistence>

      <state name="display">
        <form>/jsp/bo/process/displayrequester</form>
        <form scope="request">/jsp/bo/process/environment/displaycompostable</form>
      </state>
      <state name="check">
        <form scope="request">/jsp/bo/process/environment/checkcompostable</form>
      </state>
      <state name="print">
        <form scope="request">/jsp/bo/process/environment/printcompostable</form>
      </state>
    </process>

  </processes>

</cvqmanager>