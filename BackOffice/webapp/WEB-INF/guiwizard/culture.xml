<?xml version="1.0" encoding="UTF-8" ?>

<cvqmanager xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="managers.xsd" >
					
  <processes group="culture" caption="Culturel" icon="pictoCulture" order="5">
	<process name="eLibrary">
		<label>Inscription à la bibliothèque</label>
		<record>fr.cg95.cvq.bo.cultural.LibraryRegistrationRequestRecord</record>
		<persistence>fr.cg95.cvq.bo.cultural.LibraryRegistrationPersistence</persistence>
		
		<state name="display">
			<form>/jsp/bo/process/displayrequester</form>
			<form scope="request">/jsp/bo/process/cultural/displaylibrary</form>
		</state>
		<state name="check">
			<form scope="request">/jsp/bo/process/cultural/checklibrary</form>
		</state>
		<state name="print">
			<form scope="request">/jsp/bo/process/cultural/printlibrary</form>
		</state>
	</process>
	<process name="eReservation">
		<label>Réservation de places</label>
		<record>fr.cg95.cvq.bo.cultural.PlaceReservationRequestRecord</record>
		<persistence>fr.cg95.cvq.bo.cultural.PlaceReservationPersistence</persistence>
		
		<state name="display">
			<form>/jsp/bo/process/displayrequester</form>
			<form scope="request">/jsp/bo/process/cultural/displayreservation</form>
		</state>
		<state name="check">
			<form scope="request">/jsp/bo/process/cultural/checkreservation</form>
		</state>
		<state name="print">
			<form scope="request">/jsp/bo/process/cultural/printreservation</form>
		</state>
	</process>
  </processes>
  
</cvqmanager>
					
