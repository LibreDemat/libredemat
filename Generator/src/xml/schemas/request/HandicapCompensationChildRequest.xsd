      <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
        Demande de compensation du handicap (Enfant de moins de 20 ans)
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.libredemat.org/schema/longdesc">
        Handicap Compensation request (Less than 20 child)
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/shortdesc">
        Compensation du handicap (Enfant de moins de 20 ans)
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.libredemat.org/schema/shortdesc">
        Handicap Compensation request (Less than 20 child)
      </xs:documentation>
      <xs:appinfo>
        <model namespace="social" />
        <common>
          <namespace name="social" />
          <steps>
            <step index="-1" ref="homeFolder">
              <widget>
                <homeFolder />
              </widget>
            </step>
            <step index="0" name="subject">
              <widgets>
                <subject into="HccrSubject" />
              </widgets>
              <conditions>
                <condition name="isLessThan18" />
                <condition name="isFatherActivityReduction" />
                <condition name="isMotherActivityReduction" />
                <condition name="isReferentMadam" />
                <condition name="isReferentFamilyDependents" />
              </conditions>
            </step>
            <step index="1" name="dwelling">
              <conditions>
                <condition name="isNotPlaceOfResidence" />
                <condition name="isInEstablishmentReception" />
                <condition name="isInSocialReception" />
              </conditions>
            </step>
            <step index="2" name="socialSecurityAndPaymentAgency">
              <conditions>
                <condition name="isSocialSecurityMemberShip" />
                <condition name="isPaymentAgencyBeneficiary" />
              </conditions>
            </step>
            <step index="3" name="occupationnalAndSchoolStatus">
              <conditions>
                <condition name="isSchoolingEnrolment" />
                <condition name="isSentToSchool" />
                <condition name="isSpecializedGrade" />
                <condition name="isPartTimeSchooling" />
                <condition name="isAccompaniedHomeSchooling" />
                <condition name="isExtraCurricular" />
                <condition name="isHighSchool" />
                <condition name="isAssistanceUnderDisability" />
                <condition name="isEmployed" />
                <condition name="isUnemployed" />
                <condition name="isRegisteredAsUnemployed" />
                <condition name="isIndemnified" />
                <condition name="isElectiveFunction" />
              </conditions>
            </step>
            <step index="4" name="folders">
              <conditions>
                <condition name="isMDPH" />
                <condition name="isCOTOREP" />
                <condition name="isCDES" />
                <condition name="isOtherFolders" />
              </conditions>
            </step>
            <step index="5" name="benefits">
              <conditions>
                <condition name="isDisabilityRecognition" />
                <condition name="isProfessionalOrientation" />
                <condition name="isEducationOfDisabledChildren" />
                <condition name="isDisabilityPension" />
                <condition name="isWorkAccidentAnnuity" />
                <condition name="isSupportedByAnInstitution" />
                <condition name="isOtherBenefits" />
              </conditions>
            </step>
            <step index="6" name="aid">
              <conditions>
                <condition name="isFamilyAssistance" />
                <condition name="isHomeIntervenant" />
                <condition name="isOtherHomeIntervant" />
                <condition name="isCareServices" />
                <condition name="isCareServiceEmployer" />
                <condition name="isHousing" />
                <condition name="isTechnicalAssistance" />
                <condition name="isCustomCar" />
                <condition name="isAnimalAid" />
                <condition name="isSpecializedTransport" />
                <condition name="isProfessionals" />
                <condition name="isSocialServiceSupport" />
              </conditions>
            </step>
            <step index="7" name="health">
              <conditions>
                <condition name="isFollowedByDoctor" />
                <condition name="isFollowedByProfessional" />
                <condition name="isFollowedByHospital" />
              </conditions>
            </step>
            <step index="8" name="project">
              <conditions>
                <condition name="isProfessionalOrientationRequest" />
                <condition name="isOtherRequest" />
              </conditions>
            </step>
            <step index="9" ref="document" />
            <step index="10" ref="validation" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>
