  <xs:import namespace="http://www.cg95.fr/cvq/schema/common"
    schemaLocation="../common/CVQCommonElements.xsd" />

  <xs:element name="RemoteSupportRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande de téléassistance
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Remote support request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Demande de téléassistance
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Remote support request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="social" />
        <common>
          <namespace name="social"/>
          <steps>
            <step index="0" name="subject"/>
            <step index="1" name="contact"/>
            <step index="2" ref="document"/>
            <step index="3" ref="validation"/>
          </steps>
          <conditions>
            <condition name="isEmergency"/>
            <condition name="isFlat"/>
            <condition name="isCoupleRequest"/>
            <condition name="isOtherContact"/>
          </conditions>
        </common>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>

            <xs:element name="RsrRequestKind" type="rsr:RsrRequestKindType" default="Individual">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Type de la demande
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Type de la demande
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject"/>
                    <conditions>
                      <condition name="isCoupleRequest" type="trigger"/>
                    </conditions>
                  </common>
                  <fo>
                    <radio/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrEmergency" type="rsr:RsrEmergencyType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Urgence (Obligatoire)
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Urgence (Obligatoire)
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject"/>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrRequester" type="rsr:RsrRequesterType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Demandeur
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject"/>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrSpouse" type="rsr:RsrSpouseType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Conjoint
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Conjoint
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject"/>
                    <conditions>
                      <condition name="isCoupleRequest" type="filled" required="true"/>
                    </conditions>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrContactKind" type="rsr:RsrContactKindType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Personne à contacter pour prendre rendez-vous pour l'installation
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Type contact
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="contact"/>
                    <conditions>
                      <condition name="isOtherContact" type="trigger"/>
                    </conditions>
                  </common>
                  <fo>
                    <radio/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrFirstContact" type="rsr:RsrContactType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Premier contact
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Premier contact
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="contact"/>
                    <conditions>
                      <condition name="isOtherContact" type="filled" required="true"/>
                    </conditions>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrSecondContact" type="rsr:RsrSecondContactType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Second contact
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Second contact
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="contact"/>
                    <conditions>
                      <condition name="isOtherContact" type="filled"/>
                    </conditions>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RsrTrustee" type="rsr:RsrTrusteeType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Coordonnées de la personne dépositaire de vos clés
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Dépositaire des clés (Optionnel - Fortement recommandé)
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="contact"/>
                  </common>
                  <fo>
                    <label/>
                  </fo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="RsrRequestKindType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Type de la demande
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
