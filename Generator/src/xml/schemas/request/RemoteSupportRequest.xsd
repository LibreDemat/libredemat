            </common>
            <fo />
            <bo />
          </xs:appinfo>
        </xs:annotation>
      </xs:element>

      <xs:element name="SubjectResideWith" type="rsr:RsrSubjectResideWithType" default="Alone">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Résidence du demandeur
          </xs:documentation>
