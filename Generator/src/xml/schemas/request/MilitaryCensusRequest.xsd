                <xs:documentation xml:lang="en" source="http://www.libredemat.org/schema/shortdesc">
                  Nationality
                </xs:documentation>
                <xs:appinfo>
                  <model namespace="users" />
                  <common>
                    <step name="census" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="ChildOtherCountry" type="cvq:FullNationalityType" minOccurs="0">
              <xs:annotation>
