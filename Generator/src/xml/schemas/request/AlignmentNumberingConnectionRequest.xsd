      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande d'alignement, numérotage, raccordement
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Alignment, numbering, connection request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Alignement, numérotage, raccordement
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Alignment, numbering, connection
	  </xs:documentation>
      <xs:appinfo>
        <model namespace="urbanism" />
        <fo namespace="urban.anc">
          <display field="Requester.LastName" stage="cadastre" page="cadastre" line="1" mode="static" />
          <display field="Requester.FirstName" stage="cadastre" page="cadastre" line="2" mode="static" />
          <display field="Requester.LastName" stage="cadastre" page="display" line="1" mode="static" />
          <display field="Requester.FirstName" stage="cadastre" page="display" line="2" mode="static" />
          <display mode="labelonly" label="Adresse du compte" stage="cadastre" page="cadastre" line="3"/>
          <display field="Requester.Address" stage="cadastre" page="cadastre" line="4" 
                label="&amp;nbsp;" mode="static"/>
        </fo>
        <bo namespace="urban">
          <display field="Requester.LastName" page="checkanc" line="1" mode="static" />
          <display field="Requester.FirstName" page="checkanc" line="2" mode="static" />
          <display field="Requester.LastName" page="printanc" line="1" mode="static" />
          <display field="Requester.FirstName" page="printanc" line="2" mode="static" />
        </bo>
        <xslfo>
          <blocks-def>
            <block id="0" label="Demandeur*" column="2" />
            <block id="1" label="Identit&#x00E9; du propri&#x00E9;taire" column="2"
              displayCondition="//ancr:RequesterQuality/text() = 'Tenant'" />
            <block id="2" label="Identit&#x00E9; du propri&#x00E9;taire" column="2"
              displayCondition="//ancr:RequesterQuality/text() = 'Cabinet'" />
            <block id="3" label="Situation cadastrale de l'immeuble*" column="2" />
            <block id="4" label="Moyen de contact*" />
          </blocks-def>
          <element blockId="0" line="0" column="0" link="Requester">
            <complexTemplate />
          </element>
          <element blockId="4" line="0" column="0" link="MeansOfContact" prefixLabel="Comment souhaitez-vous &#x00EA;tre inform&#x00E9; ?">
            <complexTemplate />
          </element>
        </xslfo>
      </xs:appinfo>
    </xs:annotation>
