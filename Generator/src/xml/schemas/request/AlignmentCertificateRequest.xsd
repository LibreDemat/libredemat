      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande de certificat d'alignement
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Alignment certificate request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Certificat d'alignement
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Alignment Certificate
      </xs:documentation>
      <xs:appinfo>
        <model namespace="urbanism" />
        <fo namespace="urban.align">
          <display field="Requester.LastName" stage="cadastre" page="cadastre" line="1"
            mode="static" />
          <display field="Requester.FirstName" stage="cadastre" page="cadastre" line="2"
            mode="static" />
          <display field="Requester.LastName" stage="cadastre" page="display" line="1" mode="static" />
          <display field="Requester.FirstName" stage="cadastre" page="display" line="2"
            mode="static" />
        </fo>
        <bo namespace="urban">
          <display field="Requester.LastName" page="checkalign" line="1" mode="static" />
          <display field="Requester.FirstName" page="checkalign" line="2" mode="static" />
          <display field="Requester.LastName" page="printalign" line="1" mode="static" />
          <display field="Requester.FirstName" page="printalign" line="2" mode="static" />
        </bo>
        <xslfo>
          <blocks-def>
            <block id="0" label="Demandeur*" column="2" />
            <block id="1" label="Identité du propriétaire" column="2"
              displayCondition="//acr:RequesterQuality/text() = 'Tenant'" />
            <block id="2" label="Situation cadastrale du terrain*" column="2" />
            <block id="3" label="Moyen de contact*" />
          </blocks-def>
          <element blockId="0" line="0" column="0" link="Requester">
            <complexTemplate />
          </element>
          <element blockId="3" line="0" column="0" link="MeansOfContact"
            prefixLabel="Comment souhaitez-vous &#x00EA;tre informé ?">
            <complexTemplate />
          </element>
        </xslfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>
            <xs:element name="RequesterQuality" type="acr:AcrRequesterQualityType">
