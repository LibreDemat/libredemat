                <xs:appinfo>
                  <model namespace="request" isTiedToRequest="true" />
                  <common>
                    <step name="taxHousehold" />
                    <conditions>
                      <condition name="isTaxHouseholdCityOther" type="trigger" />
                    </conditions>
                  </common>
                  <pdf />
