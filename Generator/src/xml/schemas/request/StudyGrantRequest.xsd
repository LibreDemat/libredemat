  xmlns:cvq="http://www.cg95.fr/cvq/schema/common"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">
  <xs:import namespace="http://www.cg95.fr/cvq/schema/common"
    schemaLocation="../common/CVQCommonElements.xsd" />
  <xs:element name="StudyGrantRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Dossier de demande de bourse
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Study grant request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Demande de bourse
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Study grant request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="school" />
        <common>
          <namespace name="school" />
          <steps>
            <step index="0" name="subject">
              <conditions>
                <condition name="livesWithParents" />
              </conditions>
            </step>
            <step index="1" name="taxHousehold" />
            <step index="2" name="otherHelps">
              <conditions>
                <condition name="otherHelpObtained" />
              </conditions>
            </step>
            <step index="3" name="currentStudies">
              <conditions>
                <condition name="notInLastYear" />
                <condition name="isInSandwichCourses" />
                <condition name="isInAbroadInternship" />
                <condition name="isInOtherStudies" />
              </conditions>
            </step>
            <step index="4" name="futureSchool">
              <conditions>
                <condition name="willDoAbroadInternship" />
              </conditions>
            </step>
            <step index="5" name="calculationElements" />
            <step index="6" name="bankReference" />
            <step index="7" ref="validation" />
          </steps>
        </common>
        <xslfo>
          <blocks-def>
            <block id="0" column="2" label="Sujet" />
            <block id="1" column="2" label="Foyer fiscal" />
            <block id="2" column="2" label="Autres aides" />
            <block id="3" column="2" label="Études en cours" />
            <block id="4" column="2" label="Précisions" displayCondition="//sgr:CurrentStudies/text() = 'sandwichCourses'" />
            <block id="5" column="2" label="Précisions" displayCondition="//sgr:CurrentStudies/text() = 'abroadInternship'" />
            <block id="6" column="2" label="Précisions" displayCondition="//sgr:CurrentStudies/text() = 'otherStudies'" />
            <block id="7" column="2" label="Études futures" />
            <block id="8" column="2" label="Éléments de calcul" />
            <block id="9" column="2" label="Coordonnées bancaires" />
          </blocks-def>
          <element blockId="0" line="0" column="0" columnSpan="2" link="Subject">
            <complexTemplate />
          </element>
        </xslfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
