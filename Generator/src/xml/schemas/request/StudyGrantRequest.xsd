  <xs:simpleType name="DistanceType">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/translation/enum">
        <enum key="undetermined" value="La distance entre votre lieu de résidence et votre lieu d'études ou de stage n'a pas pu être déterminée. L'agent qui traitera votre demande pourra être amené à vous demander de préciser les informations nécessaires avant de traiter votre dossier." />
        <enum key="lessThan30kms" value="Moins de 30 kms du domicile" />
        <enum key="between30and250kms" value="Entre 30 et 250 kms du domicile" />
        <enum key="moreThan250kmsAndAbroad" value="Plus de 250 kms du domicile et à l'international" />
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/translation/enum">
        <enum key="undetermined" value="The distance bewteen home and school or internship could not be determined. The agent who will process your request may ask you required information." />
        <enum key="lessThan30kms" value="Less than 30 kms from home" />
        <enum key="between30and250kms" value="Between 30 and 250 kms from home" />
        <enum key="moreThan250kmsAndAbroad" value="More than 250 kms from home and abroad" />
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="undetermined"></xs:enumeration>
      <xs:enumeration value="lessThan30kms"></xs:enumeration>
      <xs:enumeration value="between30and250kms"></xs:enumeration>
      <xs:enumeration value="moreThan250kmsAndAbroad"></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  
