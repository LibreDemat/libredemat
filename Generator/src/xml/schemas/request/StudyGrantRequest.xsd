  <xs:element name="StudyGrantRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande de bourse Mobil'Etudes
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Study grant request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Bourse Mobil'Etudes
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Study grant request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="school" />
        <common>
          <namespace name="school" />
          <steps>
            <step index="0" name="subject">
              <widgets>
                <subject into="SubjectInformations">
                  <autofill name="subjectFilling" type="trigger" />
                </subject>
              </widgets>
            </step>
            <step index="1" name="taxHousehold">
              <conditions>
                <condition name="isTaxHouseholdCityOther" />
              </conditions>
            </step>
            <step index="2" name="otherHelps" />
            <step index="3" name="currentStudies">
              <conditions>
                <condition name="isCurrentSchoolNameOther" />
                <condition name="isInOtherStudies" />
                <condition name="makesAbroadInternship" />
              </conditions>
            </step>
            <step index="4" name="bankReference">
              <conditions>
                <condition name="isSubjectAccountHolder" />
              </conditions>
            </step>
            <step index="5" ref="document" />
            <step index="6" ref="validation" />
            <step index="7" ref="administration" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>
            <xs:element name="SubjectInformations" type="sgr:SubjectInformationsType">
              <xs:annotation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <pdf />
                  <fo />
                  <bo column="1" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="TaxHouseholdLastName" type="cvq:LastNameType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nom
                </xs:documentation>
                <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Last name
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nom
                </xs:documentation>
                <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Last name
                </xs:documentation>
                <xs:appinfo>
                  <model namespace="users" />
                  <common>
                    <step name="taxHousehold" />
                  </common>
                 <pdf />
                  <fo />
                  <bo column="1" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="TaxHouseholdFirstName" type="cvq:FirstNameType">
