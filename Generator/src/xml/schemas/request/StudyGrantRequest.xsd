<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.cg95.fr/cvq/schema/sgr"
  xmlns:sgr="http://www.cg95.fr/cvq/schema/sgr"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:cvq="http://www.cg95.fr/cvq/schema/common"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">
  
  <xs:import namespace="http://www.cg95.fr/cvq/schema/common"
    schemaLocation="../common/CVQCommonElements.xsd" />
    
  <xs:element name="StudyGrantRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande de bourse Mobil'Etudes
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Study grant request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Bourse Mobil'Etudes
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Study grant request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="school" />
        <common>
          <namespace name="school" />
          <steps>
            <step index="0" name="subject">
              <widgets>
                <subject into="SubjectInformations">
                  <autofill name="subjectFilling" type="trigger" />
                </subject>
              </widgets>
            </step>
            <step index="1" name="taxHousehold">
              <conditions>
                <condition name="isTaxHouseholdCityOther" />
              </conditions>
            </step>
            <step index="2" name="otherHelps" />
            <step index="3" name="currentStudies">
              <conditions>
                <condition name="isCurrentSchoolNameOther" />
                <condition name="isInOtherStudies" />
                <condition name="makesAbroadInternship" />
              </conditions>
            </step>
            <step index="4" name="calculationElements" />
            <step index="5" name="bankReference">
              <conditions>
                <condition name="isSubjectAccountHolder" />
              </conditions>
            </step>
            <step index="6" ref="document" />
            <step index="7" ref="validation" />
          </steps>
        </common>
        <xslfo>
          <blocks-def>
            <block id="0" column="2" label="État civil de l'étudiant" />
            <block id="1" column="2" label="Foyer fiscal de référence" />
            <block id="2" column="2" label="Autres aides obtenues" />
            <block id="3" column="2" label="Établissement scolaire d'inscription" />
            <block id="4" column="2" label="Précisions sur le diplôme préparé" 
            	displayCondition="//sgr:CurrentStudies/text() = 'otherStudies'" />
            <block id="5" column="2" label="Renseignements sur le stage à l'étranger" 
            	displayCondition="//sgr:AbroadInternship" />
            <block id="6" column="2" label="Éléments de calcul de la bourse" />
            <block id="7" column="2" label="Références bancaires de l'étudiant" />
            <block id="8" label="Moyen de contact*" />
          </blocks-def>
          <element blockId="0" line="0" column="0" columnSpan="2" link="Subject">
            <complexTemplate />
          </element>
          <element blockId="8" line="0" column="0" link="MeansOfContact"
            prefixLabel="Comment souhaitez-vous être informé ?">
            <complexTemplate />
          </element>
        </xslfo>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
