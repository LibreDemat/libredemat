            <xs:element name="Vendredi" type="dccrr:DccrrPlageHoraireVendrediType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Vendredi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Vendredi
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <!--        STEP 2 : Prise de rendez-vous         -->
            <xs:element name="ChoixTypeRendezVous" type="dccrr:RendezVousType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Vous préférez un rendez-vous :
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Vous préférez un rendez-vous :
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="TelephoneContact" type="cvq:PhoneType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  N° de téléphone sur lequel
                  nous vous contacterons
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  N° de téléphone sur lequel nous vous contacterons
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="PlageHoraireContact" type="dccrr:PlageHoraireContactType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Moment de la journée où vous préférez que nous vous contactions
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Moment de la journée où vous préférez que nous vous contactions
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="CommentaireCitoyen" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Renseignements
                  supplémentaires
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Renseignements supplémentaires
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="rendezVous" />
                  </common>
                  <fo>
                    <textarea rows="10" />
                  </fo>
                  <bo column="2">
                    <textarea rows="10" />
                  </bo>
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="600" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>

          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>

  <!--  types complexes -->
