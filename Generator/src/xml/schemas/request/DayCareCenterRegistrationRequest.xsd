<xs:schema targetNamespace="http://www.libredemat.org/schema/dccrr" xmlns:dccrr="http://www.libredemat.org/schema/dccrr"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cvq="http://www.libredemat.org/schema/common"
  elementFormDefault="qualified" attributeFormDefault="unqualified">

  <xs:import namespace="http://www.libredemat.org/schema/common" schemaLocation="../common/CVQCommonElements.xsd" />

  <xs:element name="DayCareCenterRegistrationRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
        Demande de pré-inscription en
        multi-accueil régulier ou occasionnel
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/shortdesc">
        Pré-inscription en multi-accueil
        régulier ou occasionnel
      </xs:documentation>
      <xs:appinfo>
        <model namespace="school" />
        <common>
          <namespace name="school" />
          <steps>
            <step index="-1" ref="homeFolder">
              <widget>
                <homeFolder />
              </widget>
            </step>
            <step index="0" name="subject">
              <widgets>
                <subject />
              </widgets>
              <conditions>
                <condition name="estAutreSituationActuelleMere" />
                <condition name="estAutreSituationActuellePere" />
                <condition name="estHorairesReguliersMere" />
                <condition name="estHorairesReguliersPere" />
              </conditions>
            </step>
            <step index="1" name="accueil">
              <conditions>
                <condition name="dateConnue" />
                <condition name="estIndifferent" />
                <condition name="estHorairesAccueilRegulier" />
                <condition name="estHorairesAccueilIrregulier" />
              </conditions>
            </step>
            <step index="2" name="rendezVous" />
            <step index="3" ref="document" />
            <step index="4" ref="validation" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>

    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>

            <!--        STEP 0 : SUBJECT       -->
            
            <xs:element name="DixHuitMoisEnfant" type="xs:date" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  18 mois de l'enfant
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  18 mois de l'enfant
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <bo column="1" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="InformationMere" type="dccrr:DccrrMereType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Informations complémentaires
                  sur la mère
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Informations complémentaires sur la mère
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="InformationPere" type="dccrr:DccrrPereType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Informations complémentaires
                  sur le père
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Informations complémentaires sur le père
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <!--        STEP 1 : ACCUEIL         -->
            
            <xs:element name="ModeAccueil" type="xs:boolean" default="true">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Le mode d'accueil souhaité vous est-il indifférent ? (collectif ou familial)
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Le mode d'accueil souhaité vous est-il indifférent ? (collectif ou familial)
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estIndifferent" type="trigger" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="ModeAccueilChoixUn" type="dccrr:ModeAccueilType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Mode d'accueil souhaité : choix 1
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Mode d'accueil souhaité : choix 1
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estIndifferent" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="ModeAccueilChoixDeux" type="dccrr:ModeAccueilType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Mode d'accueil souhaité : premier choix
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Mode d'accueil souhaité : second choix
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estIndifferent" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="AccueilAnterieur" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Mode d'accueil antérieur
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Mode d'accueil antérieur
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="DatePlacementAccueilRegulier" type="dccrr:DccrrDatesPlacementType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Date du placement
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Date du placement
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="ChoixHorairesAccueil" type="dccrr:ChoixHorairesAccueilType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Vous souhaitez préciser des horaires d'accueil
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Vous souhaitez préciser des horaires d'accueil
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilRegulier" type="trigger"/>
                        <condition name="estHorairesAccueilIrregulier" type="trigger"/>
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="PlageHoraireAccueilReguliere" type="dccrr:DccrrPlageHoraireType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Horaire d'accueil pour tous les jours de la semaine
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Horaire d'accueil pour tous les jours de la semaine
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilRegulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Lundi" type="dccrr:DccrrPlageHoraireLundiType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Lundi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
