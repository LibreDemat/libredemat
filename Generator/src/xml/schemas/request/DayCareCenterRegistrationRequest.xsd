                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="accueil" />
                    <conditions>
                        <condition name="estHorairesAccueilIrregulier" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="Mardi" type="dccrr:DccrrPlageHoraireMardiType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.libredemat.org/schema/longdesc">
                  Mardi
                </xs:documentation>
                <xs:documentation xml:lang="fr"
                  source="http://www.libredemat.org/schema/shortdesc">
                  Mardi
                </xs:documentation>
                <xs:appinfo>
                  <common>
