<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.cg95.fr/cvq/schema/dhr"
  xmlns:dhr="http://www.cg95.fr/cvq/schema/dhr" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:cvq="http://www.cg95.fr/cvq/schema/common" 
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">

  <xs:import namespace="http://www.cg95.fr/cvq/schema/common"
    schemaLocation="../common/CVQCommonElements.xsd" />

  <xs:element name="DomesticHelpRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
        Demande d'aide ménagère
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/longdesc">
        Domestic help request
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Demande d'aide ménagère
      </xs:documentation>
      <xs:documentation xml:lang="en" source="http://www.cg95.fr/cvq/schema/shortdesc">
        Domestic help request
      </xs:documentation>
      <xs:appinfo>
        <model namespace="social" />
        <fo namespace="social.domestichelp">
          <display field="Subject.Adult.Title" stage="subject" page="subject" line="1" mode="static" />
          <display field="Subject.Adult.FamilyStatus" stage="subject" page="subject" line="2" mode="static" />
          <display field="Subject.Adult.LastName" stage="subject" page="subject" line="3" mode="static" />
          <display field="Subject.Adult.FirstName" stage="subject" page="subject" line="4" mode="static" />
          <display field="Subject.Adult.MaidenName" stage="subject" page="subject" line="5" mode="static" />
          <display field="Subject.Adult.BirthDate" stage="subject" page="subject" line="6" label="Né(e) le" mode="static" />
          <display field="Subject.Adult.BirthPlace.City" stage="subject" page="subject" line="7" mode="static" />
		  
		  <display field="Subject.Adult.Title" stage="subject" page="display" line="1" mode="static" />
          <display field="Subject.Adult.FamilyStatus" stage="subject" page="display" line="2" mode="static" />
          <display field="Subject.Adult.LastName" stage="subject" page="display" line="3" mode="static" />
          <display field="Subject.Adult.FirstName" stage="subject" page="display" line="4" mode="static" />
          <display field="Subject.Adult.MaidenName" stage="subject" page="display" line="5" mode="static" />
          <display field="Subject.Adult.BirthDate" stage="subject" page="display" line="6" label="Né(e) le" mode="static" />
          <display field="Subject.Adult.BirthPlace.City" stage="subject" page="display" line="7" mode="static" />

          <display mode="labelonly" label="Demandeur" stage="validation" page="summarysubject" line="200" />
          <display field="Subject.Adult.Title" stage="validation" page="summarysubject" line="201" mode="static" />
          <display field="Subject.Adult.FamilyStatus" stage="validation" page="summarysubject" line="202" mode="static" />
          <display field="Subject.Adult.LastName" stage="validation" page="summarysubject" line="203" mode="static" />
          <display field="Subject.Adult.FirstName" stage="validation" page="summarysubject" line="204" mode="static" />
          <display field="Subject.Adult.MaidenName" stage="validation" page="summarysubject" line="205" mode="static" />
          <display field="Subject.Adult.BirthDate" stage="validation" page="summarysubject" line="206" label="Né(e) le" mode="static" />
          <display field="Subject.Adult.BirthPlace.City" stage="validation" page="summarysubject" line="207" mode="static" />
        </fo>
        <bo namespace="social">
          <display mode="stagetitle" label="Demandeur" page="displaydomestichelp_subject" line="200" />
          <display field="Subject.Adult.Title" page="displaydomestichelp_subject" line="201" mode="static" />
          <display field="Subject.Adult.FamilyStatus" page="displaydomestichelp_subject" line="202" mode="static" />
          <display field="Subject.Adult.LastName" page="displaydomestichelp_subject" line="203" mode="static" />
          <display field="Subject.Adult.FirstName" page="displaydomestichelp_subject" line="204" mode="static" />
          <display field="Subject.Adult.MaidenName" page="displaydomestichelp_subject" line="205" mode="static" />
          <display field="Subject.Adult.BirthDate" page="displaydomestichelp_subject" line="206" label="Né(e) le" mode="static" />
          <display field="Subject.Adult.BirthPlace.City" method="Subject.Adult.BirthCity" page="displaydomestichelp_subject" line="207"
            mode="static" />

          <display field="Subject.Adult.LastName" page="checkdomestichelp" line="201" mode="static" />
          <display field="Subject.Adult.FirstName" page="checkdomestichelp" line="202" mode="static" />

          <display field="Subject.Adult.LastName" page="printdomestichelp" line="201" mode="static" />
          <display field="Subject.Adult.FirstName" page="printdomestichelp" line="202" mode="static" />
        </bo>
        <xslfo>
          <blocks-def>
          	<block id="0" label="Demande" column="2" />
          	<block id="1" label="Référent du demandeur" column="2" />
            <block id="2" label="Demandeur" column="2" />
            <block id="3" label="Tuteur" column="2" />
            <block id="4" label="Conjoint" column="2" displayCondition="//dhr:RequesterHasSpouse/text() = 'True'"/>
            <block id="5" label="Habitation actuelle" column="2" />
            <block id="6" label="Habitation(s) antérieure(s)" column="2" />
            <block id="7" label="Ressources du demandeur" column="2" />
            <block id="8" label="Ressources du conjoint" column="2" displayCondition="//dhr:RequesterHasSpouse/text() = 'True'"/>
            <block id="9" label="Biens immobiliers" column="2" />
            <block id="10" label="Biens partage, donation ou vente" column="2" />
            <block id="11" label="Montant d'imposition" column="2" />
            <block id="12" label="Moyen de contact*" />
          </blocks-def>
          <element blockId="2" line="0" column="0" link="Requester">
            <complexTemplate />
          </element>
          <element blockId="12" line="0" column="0" link="MeansOfContact"
            prefixLabel="Comment souhaitez-vous être informé?">
            <complexTemplate />
          </element>
        </xslfo>
      </xs:appinfo>
    </xs:annotation>

    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>      
			<xs:element name="RequesterRequestKind" type="dhr:DhrRequestKindType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Type de la demande
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Type de la demande
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display field="" type="radiolist" stage="subject" page="requestkind" line="0" mode="inline"/>
              		<display field="" type="radiolist" stage="subject" page="displayrequestkind" line="0" mode="static" />
              		<display mode="labelonly" label="Demande" stage="validation" page="summaryrequestkind" line="0"/>
              		<display field="" type="radiolist" stage="validation" page="summaryrequestkind" line="1" mode="static" />
                  </fo>
            	  <bo>
            	  	<display mode="stagetitle" label="Demande" page="displaydomestichelp_requestkind" line="0" />
              		<display field="" type="select" page="displaydomestichelp_requestkind" line="1" />
            	  </bo>    
                  <xslfo>
                   <element blockId="0" line="0" column="0">
		      			<varargCheckbox displayModulo="2"/>
		    		</element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="RequesterFamilyReferent" type="dhr:RequesterFamilyReferentType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Référent du demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Référent du demandeur
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	 <display mode="labelonly" label="Référent du demandeur" stage="validation" page="summaryfamilyreferent" line="100" />       
                  </fo>
                  <bo>
                  	 <display mode="stagetitle" label="Référent du demandeur" page="displaydomestichelp_referent" line="100" />
                  </bo>
                  <xslfo>
                    <element blockId="1" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="Nationality" type="cvq:NationalityType">
              <xs:annotation>
                <xs:appinfo>
                  <model namespace="users" />
                  <fo>
                    <display field="" type="select" stage="subject" page="subject" line="8" />
                    <display field="" type="select" stage="subject" page="display" line="8" mode="static" />
                    <display field="" type="select" stage="validation" page="summarysubject" line="208" mode="static" />
                  </fo>
                  <bo>
                    <display field="" type="select" page="displaydomestichelp_subject" line="208" />
                  </bo>
                  <xslfo>
                    <element blockId="2" line="1" column="0">
                      <string />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="FranceArrivalDate" type="xs:date">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Date d'arrivée en France
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Date d'arrivée en France
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                    <display field="" stage="subject" page="subject" line="9" block="OutsideEuropeanUnion" condition="nationality == OutsideEuropeanUnion" />
                    <display field="" stage="subject" page="display" line="9" mode="static" />
                    <display field="" stage="validation" page="summarysubject" line="209" mode="static" />
                  </fo>
                  <bo>
                    <display field="" page="displaydomestichelp_subject" line="209" />
                  </bo>
                  <xslfo>
                    <element blockId="2" line="2" column="0">
                      <string />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

		 	<xs:element name="MoreThan15YearsInFrance" type="xs:boolean" default="false">
		      <xs:annotation>
		      	<xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
		            Je réside en France depuis plus de 15 ans de manière continue
		        </xs:documentation>
		        <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
		            Je réside en France depuis plus de 15 ans de manière continue
		        </xs:documentation>
		         <xs:appinfo>
		         <fo>
                    <display field="" type="yesno" stage="subject" page="subject" line="10" block="OutsideEuropeanUnion" condition="nationality == OutsideEuropeanUnion" mode="inline"/>
                    <display field="" type="yesno" stage="subject" page="display" line="10" mode="static" />
                    <display field="" type="yesno" stage="validation" page="summarysubject" line="210" mode="static" />
                  </fo>
                  <bo>
                    <display field="" type="yesno" page="displaydomestichelp_subject" line="210" />
                  </bo>
		         	<xslfo>
		              <element blockId="2" line="3" column="0">
		                <yesnoCheckbox />
		              </element>
		            </xslfo>
		          </xs:appinfo>
		        </xs:annotation>
		      </xs:element>
            
            <xs:element name="RequesterPensionPlan" type="dhr:DhrPensionPlanType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Régime de retraite principal du demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Régime de retraite principal
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                    <display field=""  type="select" stage="subject" page="subject" line="11" />
                    <display field=""  type="select" stage="subject" page="display" line="11" mode="static" />
                    <display field=""  type="select" stage="validation" page="summarysubject" line="211" mode="static" />
                  </fo>
                  <bo>
                    <display field="" type="select" page="displaydomestichelp_subject" line="211" />
                  </bo>
                  <xslfo>
                    <element blockId="2" line="4" column="0" >
                      <string />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="PensionPlanPrecision" >
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Précision si régime de retraite principal est Autre
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Préciser
                </xs:documentation>
                <xs:appinfo>
                <fo>
                    <display field="" stage="subject" page="subject" line="12" block="Other" condition="requesterPensionPlan == Other" />
                    <display field="" stage="subject" page="display" line="12" mode="static" />
                    <display field="" stage="validation" page="summarysubject" line="212" mode="static" />
                  </fo>
                  <bo>
                    <display field="" page="displaydomestichelp_subject" line="212" />
                  </bo>
                  <xslfo>
                    <element blockId="2" line="5" column="0" columnSpan="2">
                      <string valueWeight="3"/>
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
          		<xs:restriction base="xs:string">
            	  <xs:maxLength value="50" />
          		</xs:restriction>
        		  </xs:simpleType>
            </xs:element>
            
            <xs:element name="ComplementaryPensionPlanPrecision" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Régime de retraite complémentaire
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Régime de retraite complémentaire
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                    <display field="" stage="subject" page="subject" line="13" />
                    <display field="" stage="subject" page="display" line="13" mode="static" />
                    <display field="" stage="validation" page="summarysubject" line="213" mode="static" />
                  </fo>
                  <bo>
                    <display field="" page="displaydomestichelp_subject" line="213" />
                  </bo>
                  <xslfo>
                    <element blockId="2" line="6" column="0" columnSpan="2">
                      <string valueWeight="3"/>
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
          		<xs:restriction base="xs:string">
            		<xs:maxLength value="50" />
          		</xs:restriction>
        	  </xs:simpleType>
            </xs:element>

            <xs:element name="RequesterSpouse" type="dhr:RequesterSpouseType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Informations administratives du conjoint
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Informations administratives du conjoint
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Conjoint" stage="validation" page="summaryspouse" line="300" />
                  </fo>
                  <bo>
                  	 <display mode="stagetitle" label="Conjoint" page="displaydomestichelp_spouse" line="300" />
                  </bo>
                  <xslfo>
                    <element blockId="4" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="CurrentDwelling" type="dhr:CurrentDwellingType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Habitation actuelle
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Habitation actuelle
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Habitation actuelle" stage="validation" page="summarycurrentdwelling" line="400" />         
                  </fo>
                  <bo>
                  	<display mode="stagetitle" label="Habitation actuelle" page="displaydomestichelp_currentdwelling" line="400" />
                  </bo>
                  <xslfo>
                    <element blockId="5" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>

            </xs:element>
			  
            <xs:element name="PreviousDwelling" type="dhr:DhrPreviousDwellingType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Habitation(s) antérieure(s) du demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Habitation(s)
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	 <display mode="labelonly" label="Habitation antérieure" stage="validation" page="summarypreviousdwelling" line="500"/>
                  </fo>
                  <bo>
                  	<display mode="stagetitle" label="Habitation antérieure" page="displaydomestichelp_previousdwelling" line="500" />
                  </bo>
                  <xslfo>
                    <element blockId="6" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
			
            <xs:element name="RequesterSituation" type="dhr:RequesterSituationType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Situation du demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Situation du demandeur
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Tuteur" stage="validation" page="summarytutor" line="214" />
                  </fo>
                  <bo>
                  	<display mode="stagetitle" label="Tuteur" page="displaydomestichelp_tutor" line="214" />
                  </bo>
                  <xslfo>
                    <element blockId="3" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            
            <xs:element name="RequesterIncomes" type="dhr:RequesterIncomesType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Ressources du demandeur
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Ressources du demandeur
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Resources du demandeur" stage="validation" page="summarysubjectincomes" line="600"/>
                  </fo>
                  <bo>
                  	<display mode="stagetitle" label="Ressources du demandeur" page="displaydomestichelp_requesterincomes" line="600" />
                  </bo>
                  <xslfo>
                    <element blockId="7" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RequesterSpouseIncomes" type="dhr:SpouseIncomesType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Ressources du conjoint
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Ressources du conjoint
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Resources du conjoint" stage="validation" page="summaryspouseincomes" line="700"/>
                  </fo>
                  <bo>
                  	 <display mode="stagetitle" label="Ressources du conjoint" page="displaydomestichelp_spouseincomes" line="700" />
                  </bo>
                  <xslfo>
                    <element blockId="8" line="0" column="0">
                      <localComplexElement />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="RealAssets" type="dhr:DhrRealAssetType" minOccurs="0"
              maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Bien(s) immobilier(s) du foyer
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Bien(s)
                </xs:documentation>

                <xs:appinfo>
                  <fo>
                    <display mode="labelonly" label="Biens immobiliers" stage="validation" page="summaryrealassets" line="800" />
                    <display type="list(assets)" stage="validation" page="summaryrealassets" line="801"
                      mode="static">
                      <display field="RealAssetAddress" line="802" mode="static" />
                      <display field="RealAssetValue" line="803" mode="static" />
                      <display field="RealAssetNetFloorArea" line="804" mode="static" />
                    </display>
                  </fo>
                  <bo>
          			<display mode="stagetitle" label="Biens Immobiliers" page="displaydomestichelp_realassets" line="800" />
                    <display field="" type="list" page="displaydomestichelp_realassets" line="801" class="DhrRealAssets" mode="static">
                      <display field="RealAssetAddress" line="802" mode="static" />
                      <display field="RealAssetValue" line="803" mode="static" />
                      <display field="RealAssetNetFloorArea" line="804" mode="static" />
                    </display>
                  </bo>
                  <xslfo>
                    <element blockId="9" line="0" column="0">
                      <localComplexElement withTotal="true" />
                    </element>
                  </xslfo>
                </xs:appinfo>

              </xs:annotation>
            </xs:element>

            <xs:element name="RealAssetsValuesTotal" type="xs:positiveInteger" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Valeur totale des biens immobiliers
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Valeur totale des biens immobiliers
                </xs:documentation>
                <xs:appinfo>
                  <bo>
                    <display field="" page="displaydomestichelp_realassets" line="806" mode="static" />
                  </bo>
                  <xslfo>
                    <element blockId="9" line="4" column="0">
                      <string />
                    </element>
                  </xslfo>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>

            <xs:element name="NotRealAssets" type="dhr:DhrNotRealAssetType" minOccurs="0"
              maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Biens partage, donation ou vente 
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Biens partage, donation ou vente 
                </xs:documentation>
                <xs:appinfo>
                  <fo>
                  	<display mode="labelonly" label="Biens partage, donation ou vente" stage="validation" page="summarynotrealassets" line="900" /> 
                    <display type="list(notrealasset)" stage="validation" page="summarynotrealassets" line="901"
                      mode="static">
                      <display field="AssetType" type="select" line="902" mode="static" />
                      <display field="AssetKind" type="select" line="903" mode="static" />
                      <display field="AssetAddress" line="904" mode="static" />
                      <display field="AssetBeneficiaryName" line="905" mode="static" />
                      <display field="AssetBeneficiaryFirstName" line="906" mode="static" />
                      <display field="AssetBeneficiaryAddress" line="907" mode="static" />
                      <display field="AssetValue" line="908" mode="static" />
                      <display field="AssetDate" line="909" mode="static" />
                      <display field="AssetNotaryName" line="910" mode="static" />
                      <display field="AssetNotaryAddress" line="911" mode="static" />
