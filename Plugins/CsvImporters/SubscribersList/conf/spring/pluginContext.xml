<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean id="subscribersListCsvImporter"
    class="fr.capwebct.capdemat.plugins.csvimporters.subscriberslist.service.SubscribersListService"
    init-method="init">
    <property name="label" value="SubscribersList"></property>
    <property name="localAuthorityRegistry">
      <ref bean="localAuthorityRegistry" />
    </property>
    <property name="placeReservationRequestService">
      <ref bean="placeReservationRequestService"/>
    </property>
  </bean>

  
  <bean class="fr.cg95.cvq.platform.PluginBeanFactoryPostProcessor">
    <property name="extensionBeanName" value="csvParserService" />
    <property name="propertyName" value="importers" />
    <property name="pluginBeanName" value="subscribersListCsvImporter" />
  </bean>
  
  
</beans>
