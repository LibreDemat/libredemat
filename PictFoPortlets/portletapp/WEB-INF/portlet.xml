<?xml version="1.0" encoding="UTF-8"?>
<portlet-app
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0">

	<portlet>
		<portlet-name>fo-login</portlet-name>
		<display-name>Authentification E-citoyen</display-name>
		<portlet-class>
			org.springframework.web.portlet.DispatcherPortlet
		</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>WEB-INF/cvq-fo-login-context.xml</value>
		</init-param>
		<init-param>
			<name>viewRendererServlet</name>
			<value>/WEB-INF/servlet/view</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>Authentification E-citoyen</title>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>title</name>
				<value>Authentification E-citoyen</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
	</portlet>

