    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="GetRequests">
      <soap:operation soapAction=""/>
      <wsdl:input name="GetRequestsRequest">
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="GetRequestsResponse">
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="AckRequests">
      <soap:operation soapAction=""/>
      <wsdl:input name="AckRequestsRequest">
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="AckRequestsResponse">
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="requestsService">
    <wsdl:port binding="schema:requestsBinding" name="requestsPort">
      <soap:address location="/wsdl"/>
    </wsdl:port>
  </wsdl:service>
