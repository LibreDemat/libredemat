  <xs:element name="GetRequestsRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="O" name="RequestTypeLabel" type="xs:string">
          <xs:annotation>
            <xs:documentation>
              The label of the request types. If not provided, all authorized 
	      request types will be returned.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DateFrom" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>
              The date to search from. 
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="DateTo" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>
              The date to search to. Defaults to today (included) if not provided.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element default="Validated" name="State" type="common:RequestStateType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="GetRequestsResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element minOccurs="0" name="Error" type="xs:string"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="Request" type="common:RequestType"/>          
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="AckRequestsRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="AckElements" type="capdemat:AckRequestType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="AckRequestsResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Accomplished" type="xs:boolean"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:complexType name="AckRequestType">
    <xs:sequence>
      <xs:element name="RequestId" type="xs:long">
        <xs:annotation>
          <xs:documentation>
            Processed request identifier
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Erroneous" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            Error flag
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>  

</xs:schema>
  </wsdl:types>
  <wsdl:message name="GetRequestsResponse">
    <wsdl:part element="schema:GetRequestsResponse" name="GetRequestsResponse"/>
  </wsdl:message>
  <wsdl:message name="GetRequestsRequest">
