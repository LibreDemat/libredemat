  </wsdl:message>
  <wsdl:message name="AckRequestsRequest">
    <wsdl:part element="schema:AckRequestsRequest" name="AckRequestsRequest"/>
  </wsdl:message>
  <wsdl:portType name="requests">
    <wsdl:operation name="GetRequests">
      <wsdl:input message="schema:GetRequestsRequest" name="GetRequestsRequest"/>
      <wsdl:output message="schema:GetRequestsResponse" name="GetRequestsResponse"/>
    </wsdl:operation>
    <wsdl:operation name="AckRequests">
      <wsdl:input message="schema:AckRequestsRequest" name="AckRequestsRequest"/>
      <wsdl:output message="schema:AckRequestsResponse" name="AckRequestsResponse"/>
    </wsdl:operation>
  </wsdl:portType>
